{
    "name": "symfony/website-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A skeleton to start a new Symfony website",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cocur/slugify": "^4.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "helios-ag/fm-elfinder-bundle": "^10.0",
        "knplabs/knp-gaufrette-bundle": "^0.7.1",
        "knplabs/knp-paginator-bundle": "^5.3",
        "liip/imagine-bundle": "^2.3",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.1.*",
        "symfony/console": "*",
        "symfony/debug-bundle": "5.1.*",
        "symfony/doctrine-messenger": "5.1.*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/security-csrf": "5.1.*",
        "symfony/serializer-pack": "*",
        "symfony/stopwatch": "5.1.*",
        "symfony/string": "*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/translation": "*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "*",
        "symfony/var-dumper": "5.1.*",
        "symfony/web-link": "*",
        "symfony/web-profiler-bundle": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "*",
        "symfonycasts/reset-password-bundle": "^1.1",
        "symfonycasts/verify-email-bundle": "^1.0",
        "twig/intl-extra": "^3.1",
        "twig/string-extra": "^3.0",
        "vich/uploader-bundle": "^1.15"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/maker-bundle": "^1.0",
        "symfony/test-pack": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "elfinder:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "5.1.*"
        }
    }
}
